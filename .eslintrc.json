{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:eslint-plugin-next-on-pages/recommended"
  ],
  "plugins": ["@typescript-eslint", "eslint-plugin-next-on-pages"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
